<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fourthpage">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/fourthbar"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:background="?attr/colorPrimary"
        android:backgroundTint="@android:color/holo_red_light"
        android:minHeight="?attr/actionBarSize"

        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="FORM PREVIEW"
        app:titleTextColor="@color/white" />

    <TableLayout
        android:id="@+id/tl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fourthbar">

        <TableRow
            android:id="@+id/t1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView

                android:layout_width="56dp"
                android:layout_height="match_parent"
                android:padding="10sp"
                android:text="NAME "
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold">

            </TextView>

            <TextView
                android:id="@+id/dispname"
                android:layout_width="241dp"
                android:layout_height="match_parent"
                android:padding="10sp"

                android:text=" DISPLAY NAME"
                android:textAlignment="center"
                android:textSize="20sp"
                android:textStyle="bold">

            </TextView>

        </TableRow>

        <TableRow
android:id="@+id/t2"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView

                android:id="@+id/yearr"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:padding="10sp"
                android:text="YEAR "
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold">

            </TextView>

            <TextView
                android:id="@+id/year"
                android:layout_width="109dp"
                android:layout_height="match_parent"
                android:padding="10sp"
                android:text=" DISPLAY YEAR"
                android:textAlignment="center"
                android:textSize="20sp"
                android:textStyle="bold">

            </TextView>

        </TableRow>

        <TableRow
            android:id="@+id/t3"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView


                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:padding="10sp"
                android:text="EMAIL"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold">

            </TextView>

            <TextView
                android:id="@+id/emai"
                android:layout_width="295dp"
                android:layout_height="match_parent"
                android:padding="10sp"
                android:text=" DISPLAY MAIL"

                android:textSize="16sp"
                android:textStyle="bold">

            </TextView>

        </TableRow>

        <TableRow
            android:id="@+id/t4"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView


                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:padding="10sp"
                android:text="GENDER "
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold">

            </TextView>

            <TextView
                android:id="@+id/gender"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:padding="10sp"
                android:text=" DISPLAY GEN"
                android:textAlignment="center"
                android:textSize="20sp"
                android:textStyle="bold">

            </TextView>

        </TableRow>

        <TableRow
            android:id="@+id/t5"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView


                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:padding="10sp"
                android:text="MOBILE"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold">

            </TextView>

            <TextView
                android:id="@+id/mob"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:padding="10sp"
                android:text=" DISPLAY mobile"
                android:textAlignment="center"
                android:textSize="20sp"
                android:textStyle="bold">

            </TextView>
        </TableRow>

        <TableRow
            android:id="@+id/t6"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView


                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:padding="10sp"
                android:text="MENTOR"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold">

            </TextView>

            <TextView
                android:id="@+id/men"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:padding="10sp"
                android:text=" DISPLAY MEN "
                android:textAlignment="center"
                android:textSize="20sp"
                android:textStyle="bold">

            </TextView>
        </TableRow>
    </TableLayout>




</androidx.constraintlayout.widget.ConstraintLayout>